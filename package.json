{
   "name": "flive-app-packages",
   "private": true,
   "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:api-client && npm run build:api-middleware && npm run build:api-redux-helpers && npm run build:router-transitions-middleware",
    "build:api-client": "npx babel ./packages/flive-app-api-client/src --out-dir ./packages/flive-app-api-client/lib",
